$noop(Valence tags: Valence -number-)
$set(_tempA,%ab:hi:mood_happy:happy%)
$set(_tempB,$get(ab:hi:mood_sad:not sad))
$if(%_tempA%,
$if(%_tempB%,
	$if($in(%_tempA%,-),
		$set(_tempA,0),
		$if($eq($substr(%_tempA%,2,3),0),
			$set(_tempA,$substr(%_tempA%,3,4)),
			$set(_tempA,$substr(%_tempA%,2,4))
		)
	)
	$if($in(%_tempB%,-),
		$set(_tempB,0),
		$if($eq($substr(%_tempB%,2,3),0),
			$set(_tempB,$substr(%_tempB%,3,4)),
			$set(_tempB,$substr(%_tempB%,2,4))
		)
	)
	$set(valence,$div($add(%_tempA%,%_tempB%),2))
))

